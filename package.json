{
  "name": "nextjs-shadcn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "dev:3005": "next dev --port 3005",
    "build": "npm run db:generate && next build",
    "start": "next start -H 0.0.0.0",
    "db:generate": "npx prisma generate || echo 'Prisma not configured, skipping...'",
    "db:push": "bunx prisma db push || echo 'Prisma not configured'",
    "db:migrate": "bunx prisma migrate dev || echo 'Prisma not configured'",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix && npx tsc --noEmit",
    "format": "bunx prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check:lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "check:format": "bunx prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "jest",
    "build:debug": "node scripts/build-with-logging.js",
    "postinstall": "npm run db:generate",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.23.9",
    "lucide-react": "^0.475.0",
    "next": "^15.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-player": "^3.3.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "undici": "^7.13.0",
    "vercel": "^44.2.5",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "^15.4.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.17.50",
    "@types/react": "^18.3.22",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "eslint-config-next": "15.1.7",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.2",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
